spring:
  application:
    name: VoicepackPlatform

  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true

  datasource:
    url: your-supabase-jdbc-url

server:
  port: 8080

# AI Model Service Configuration
ai:
  model:
    service:
      url: https://your-cloudrun-url

# Supabase API
supabase:
  url: your-supabase-url
  key: your-supabase-anon-key
