spring.application.name=VoicepackPlatform

# AI Model Service Configuration
ai.model.service.voicepack_creation=https://your-cloudrun-url
ai.model.service.voicepack_synthesis=https://your-cloudrun_url

supabase.url=your-supabase-url
supabase.key=your-supabase-anon-key

# AWS S3 설정
aws.s3.access-key=${AWS_ACCESS_KEY:your-access-key}
aws.s3.secret-key=${AWS_SECRET_KEY:your-secret-key}
aws.s3.region=${AWS_REGION:ap-northeast-2}
aws.s3.bucket-name=${AWS_BUCKET_NAME:your-bucket-name}
